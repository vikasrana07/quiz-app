{
  "name": "admin-app",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "start": "nx serve",
    "start:fe": "ng serve client",
    "start:be": "ng serve api",
    "build": "nx build client && nx build api",
    "update": "nx migrate latest",
    "migrations": "nx migrate --run-migrations",
    "lint": "nx workspace-lint && ng lint",
    "test": "nx test",
    "pretty": "prettier --write \"./**/*.{js,jsx,ts,tsx,json}\"",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "typeorm:run-migrations": "npm run typeorm migration:run -- -d ./typeOrm.config.ts",
    "typeorm:generate-migration": "npm run typeorm -- -d ./typeOrm.config.ts migration:generate ./apps/api/src/migrations/$npm_config_name",
    "typeorm:create-migration": "npm run typeorm -- migration:create ./apps/api/src/migrations/$npm_config_name",
    "typeorm:revert-migration": "npm run typeorm -- -d ./typeOrm.config.ts migration:revert"
  },
  "private": true,
